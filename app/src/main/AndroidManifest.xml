    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/jd_buy_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.MainActivity" />
        <activity android:name=".view.activity.TranPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.LoginActivity" />
        <activity android:name=".view.activity.WebActivity" />
        <activity android:name=".view.activity.CustomCapatrueActivity" />
        <activity android:name=".view.activity.SearchActivity" />
        <activity android:name=".view.activity.ProductListActivity" />
        <activity android:name=".view.activity.DetailActivity" />
        <activity android:name=".view.activity.ShoppCartActivity" />
        <activity android:name=".view.activity.RegisterActivity" />
        <activity android:name=".view.activity.PersonMationActivity"></activity>
    </application>

</manifest>